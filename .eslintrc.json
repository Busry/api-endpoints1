{
    "extends": "airbnb",
    "parserOptions": {
      "ecmaVersion": 6
    }, 
    "env": {
      "node": true,
      "browser": true  
    },
    
    "rules": {
      "comma-dangle": ["error", "never"],
      "no-unused-vars": ["error", { "var": "local", "args": "none" }],
      "quotes": "off"
    }
}
 